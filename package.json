{
  "name": "feature-flags-launch-darkly",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@auth0/auth0-react": "1.10.2",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mui/icons-material": "5.11.0",
    "@mui/material": "5.11.6",
    "@mui/x-data-grid": "5.17.23",
    "@typescript-eslint/parser": "5.49.0",
    "axios": "1.2.3",
    "launchdarkly-react-client-sdk": "3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.7.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@mui/types": "7.2.3",
    "@trivago/prettier-plugin-sort-imports": "4.0.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-standard-with-typescript": "32.0.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-n": "15.0.0",
    "eslint-plugin-no-inline-styles": "1.0.5",
    "eslint-plugin-no-relative-import-paths": "1.5.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.32.1",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.3",
    "typescript": "4.9.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint:fix": "eslint --fix",
    "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint:staged": "eslint --fix --ext .js,.jsx,.ts,.tsx --max-warnings=0 --no-ignore",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "clean-install": "rm -rf node_modules && rm yarn.lock && yarn install",
    "check-types": "yarn tsc",
    "validate": "npm-run-all --parallel lint check-types build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn run lint:staged"
    ],
    "*.{html,json,css}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}